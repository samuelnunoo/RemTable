{
  "name": "rem-table",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "docker:build": "docker build . -t rem-table-image",
    "docker:remove": "docker image rm rem-table-image --force",
    "docker:watch": " webpack serve --host 0.0.0.0 --config config/webpack/webpack.dev.js",
    "docker:dev": "docker run --rm  -v %cd%:/app -v /app/node_modules -p 8080:8080 --name rem-table-container rem-table-image",
    "local": "webpack serve --config config/webpack/webpack.dev.js",
    "deploy": "webpack --config config/webpack/webpack.prod.js && gh-pages -d dist",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "homepage": "https://samuelnunoo.github.io/RemTable",
  "dependencies": {
    "@babel/preset-typescript": "^7.12.7",
    "copy-webpack-plugin": "^7.0.0",
    "core-js": "^3.6.5",
    "css-loader": "^5.0.1",
    "html-webpack-plugin": "^5.0.0-alpha.9",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.1",
    "tabulator-tables": "4.8.1",
    "typescript": "^4.1.3",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.6",
    "vue-style-loader": "^4.1.2",
    "vue-tabulator": "^1.3.0",
    "vueloader": "^0.0.3",
    "vuetify": "^2.4.3",
    "webpack": "^5.15.0",
    "webpack-blocks-ts": "^2.0.1",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@storybook/addon-actions": "^6.1.15",
    "@storybook/addon-essentials": "^6.1.15",
    "@storybook/addon-links": "^6.1.15",
    "@storybook/vue": "^6.1.15",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "deepmerge": "^4.2.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.32.5",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
